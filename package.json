{
	"name": "hitori",
	"version": "1.0.0",
	"description": "Hitori solver",
	"author": "Sondre Gjellestad <sondre.gjellestad@hotmail.com> (https://sondregjellestad.space)",
	"homepage": "https://github.com/sondregj/hitori#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sondregj/hitori.git"
	},
	"license": "MIT",
	"main": "dist/index.js",
	"types": "dist/index.d.js",
	"scripts": {
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "tslint -p tsconfig.json",
		"test": "ava",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"prepublishOnly": "npm test && npm run lint && npm run build"
	},
	"bugs": {
		"url": "https://github.com/sondregj/hitori/issues"
	},
	"devDependencies": {
		"ava": "^2.3.0",
		"dotenv": "^8.1.0",
		"tslint": "^5.20.0",
		"typescript": "^3.6.3",
		"prettier": "1.18.2",
		"tslint-config-prettier": "1.18.0"
	},
	"files": [
		"dist/**/*"
	],
	"keywords": [
		"hitori"
	]
}
